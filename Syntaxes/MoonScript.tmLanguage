			<key>name</key>
			<string>support.function.library.lua.love</string>
		</dict>
	</array>

	<key>scopeName</key>
	<string>source.moonscript</string>
	<key>uuid</key>
	<string>D363822C-639B-4450-A21A-F45643A6940F</string>
