		<dict>
			<key>match</key>
			<string>\b(and|or|not)\b</string>
			<key>name</key>
			<string>keyword.operator.lua</string>
		</dict>

		<dict>
			<key>match</key>
			<string>[a-zA-Z_]\w*\s*(?=:)</string>
			<key>name</key>
			<string>entity.name.function</string>
		</dict>

		<dict>
			<key>match</key>
			<string>[a-zA-Z_]\w*\s*(?==\s*?\(?.*\)?[-=]>)</string>
			<key>name</key>
			<string>entity.name.function</string>
		</dict>

		<dict>
			<key>match</key>
			<string>\(|\)|\!</string>
			<key>name</key>
			<string>entity.name.function</string>
		</dict>

		<dict>
			<key>match</key>
			<string>\+|-|%|#|\*|\/|\^|==?|~=|!=|\\|:|,|;|\.|&lt;=?|&gt;=?|(?&lt;!\.)\.{2}(?!\.)</string>
			<key>name</key>
			<string>keyword.operator.lua</string>
